 "html": "<div class=\"container\">\n    {% set supplier = frappe.get_doc(\"TSAI Supplier\",doc.supplier_code) %}\n    {% for i in doc.invoice_items %}\n    {% set pkg_std = i.key_qty/i.packing_standard %}\n    {% for d in range(pkg_std|int)%}\n    <div class=\"card\">\n        <table border=\"2\">\n            <tr>\n                <td colspan=\"2\" nowrap><b>TAG CARD For Thai Summit Autoparts India</b></td>\n                <td colspan=\"1\">Location :</td>\n            </tr>\n\n            <tr>\n                <td colspan=\"3\">\n                    <center><b>{{supplier.supplier_name}}</b></center>\n                </td>\n            </tr>\n\n            <tr>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\"><b>MAT No :</b></td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\"> <b>Part No:</b></td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;\"><b>Part Name:</b></td>\n            </tr>\n            <tr>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\">{{i.mat_no}}</td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\">{{i.parts_no}}</td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;\">{{i.parts_name}}</td>\n            </tr>\n\n            <tr>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\"><b>QTY</b></td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;border-right-style: hidden\"><b>Invoice No</b></td>\n                <td colspan=\"1\" style=\"border-bottom-style: hidden;\"><b>Invoice Date</b></td>\n            </tr>\n\n            <tr>\n                <td colspan=\"1\" style=\"border-right-style: hidden\">{{i.packing_standard}}</td>\n                <td colspan=\"1\" style=\"border-right-style: hidden\">{{doc.invoice_no}}</td>\n   
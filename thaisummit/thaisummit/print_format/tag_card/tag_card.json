 "html": "<style>\ntable, th, td {\n  border: 1px solid black;\n  font-size:12px;\n}\n</style>\n{% set doc_name = doc.mat_number %}\n{% set link = '<img style=width:70%; height=\"50\" alt=\"\" src=\"https://barcodeapi.org/api/128/'+doc_name+'\">' %}\n\n\n\n    {% for i in doc.table_9 %}\n    <table width= 50% border=1>\n    <tr><td style=text-align:center><b>\n    ITEM CARD NO:\n    {{ i.item_card_no }}\n   \n    \n     \n</b>\n   </td>\n   </tr>\n   <tr>\n       <td style=text-align:center><b>Date:{{doc.date or '' }},Time:{{doc.time or \"\" }},Shift:{{doc.shift or \"\"}}</b></td>\n   </tr>\n   \n</table>\n<table width= 50%>\n    <tr>\n       <td style=width:70%;text-align:center><b>Part No</b></td>\n       <td style=width:30%; rowspan=4><img style=width:100%; height=42% alt=\"\" src=\"https://api.qrserver.com/v1/create-qr-code/?data={{i.item_card_no}},{{doc.user_name}}\"></td>\n   </tr>\n   <tr>\n       <td style=text-align:center>{{doc.part_number or '' }}</td>\n   </tr>\n   <tr>\n       <td style=text-align:center><b>Part Name</b></td>\n   </tr>\n   <tr>\n       <td style=text-align:center>{{doc.mat_name or ''}}</td>\n   </tr>\n   </table>\n   <table width= 50% border=1>\n          <tr><td>Mat Number</td><td>{{doc.mat_number or ''}}</td><td>QTY</td><td>{{doc.quantity or '' }}</td></tr>\n        <tr><td>Sup.Code</td><td>{{doc.supcode or '' }}</td><td>User Code</td><td>{{doc.user_code or '' }}</td></tr>\n        <tr><td>Sup.Name</td><td>TSAI</td><td>User Name</td><td>{{doc.user_name or ''}}</td></tr>\n   </table>\n\n<table width=50%>\n    <tr>\n        <td>Prod<br><br><br><br>Date/Sign</td>\n        <td>PQA<br><br><br><br>Date/Sign</td>\n        <td>FQA<br><br><br><br>Date/Sign</td>\n        <td>Sales<br><br><br><br>Date/Sign</td>\n    </tr>\n    \n   <tr><td colspan=4>&ensp;&ensp;&ensp;&ensp;&ensp;&ensp;&ensp;&ensp;&ensp;{{link}}</td></tr>\n   <tr><td colspan=4 style=text-align:center><b>TSAI</b></td></tr>\n\n</table>\n<br>\n<br>\n<br>\n<br>\n{% endfor %}\n\n\n\n\